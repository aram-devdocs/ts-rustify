{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./typescript-sdk",
    "declaration": true
  },
  "include": ["typescript-sdk/**/*", "ts-wrapper/**/*"],
  "exclude": ["node_modules"]
}
